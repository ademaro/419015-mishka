    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "description": "Личный проект «Мишка» от HTML Academy",
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "7.1.x",
    "browser-sync": "2.18.x",
    "del": "^3.0.0",
    "gulp": "3.9.x",
    "gulp-csso": "^3.0.0",
    "gulp-imagemin": "^3.4.0",
    "gulp-less": "3.3.x",
    "gulp-plumber": "1.1.x",
    "gulp-postcss": "7.0.x",
    "gulp-posthtml": "^3.0.0",
    "gulp-svgstore": "^6.1.0",
    "gulp-uglify": "^3.0.0",
